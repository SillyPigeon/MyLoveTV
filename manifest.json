 {
    "name": "myLoveTV Extensions",
    "description" : "The simple hbbtv Extension",
    "version": "1.0",
    "manifest_version": 2,

    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]

  }